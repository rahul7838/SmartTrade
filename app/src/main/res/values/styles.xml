<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppToolbar.Title">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="AppToolbar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?android:attr/actionBarSize</item>
        <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="ButtonContained" parent="android:Widget.Material.Button">
        <item name="android:background">@drawable/button_rounded_dark_blue</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="ButtonBorderless" parent="android:Widget.Material.Button.Borderless.Colored">


    </style>
    <style name="ButtonBorderless1" parent="@style/Widget.MaterialComponents.Button.OutlinedButton"/>

</resources>